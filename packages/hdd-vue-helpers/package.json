    "@vue/tsconfig": "^0.7.0",
    "ajv": "^8.17.1",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.9",
    "vue": "^3.5.13"
  }
