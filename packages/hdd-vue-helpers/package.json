{
  "name": "@hassandomedenea/hdd-vue-helpers",
  "type": "module",
  "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
